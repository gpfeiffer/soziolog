<h1>Users</h1>

<table>
  <tr>
    <td>email</td>
    <td>reset</td>
    <td>signed in</td>
    <td>#</td>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><code><%= link_to user.email, user %></code></td>
    <td><%= user.reset_password_sent_at %></td>
    <td><%= user.current_sign_in_at %></td>
    <td align="right"><%= user.sign_in_count %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
